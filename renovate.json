{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "baseBranches": ["master", "test-branch"],
  "recreateClosed": true,
  "useBaseBranchConfig": "merge",
  "packageRules": [
    {
      "groupName": "new test group",
      "matchPackageNames": ["popper.js"],
      "followTag": "latest",
      "separateMajorMinor": "false"
    }
  ]
}
